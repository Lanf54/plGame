extends layout

block content
	div.container
		div.page-header.text-center
			h1
				span.fa.fa-anchor
				span Profile Page
			a(href="/logout").btn.btn-default.btn-sm Logout
		div.row
			<!-- LOCAL INFORMATION -->
			div.col-sm-6
				div.well
					h3
						span.fa.fa-user
						span Local
					p
						strong id : #{user._id}
						br
						strong email : #{user.local.email}
						br
						strong password : #{user.local.password}